{
    "name": "typescript-express-ioc",
    "version": "0.1.0",
    "description": "A starting point for Node.js express apps with TypeScript",
    "repository": {
        "type": "git",
        "url": "https://github.com/tnkemdilim/typescript-express-ioc"
    },
    "author": "Tochukwu Nkemdilim",
    "license": "MIT",
    "scripts": {
        "lint": "eslint \"**/*.{js,ts}\" --quiet --fix",
        "build": "tsc --noEmit && npm run lint",
        "serve": "node dist/server.js",
        "start": "npm run build && npm run serve",
        "watch": "tsc -w",
        "start:nodemon": "npm run build && npm run watch:nodemon",
        "watch:nodemon": "nodemon dist/server.js",
        "watch:test": "npm run test -- --watchAll",
        "test": "jest --forceExit --coverage --verbose --detectOpenHandles",
        "start:concurrent": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch\" \"PUPPETEER_PRODUCT=chrome npm run watch:nodemon\""
    },
    "dependencies": {
        "@sideway/address": "^4.1.0",
        "body-parser": "^1.19.0",
        "cloudinary": "^1.23.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "errorhandler": "^1.5.1",
        "express": "^4.17.1",
        "ioredis": "^4.19.4",
        "prom-client": "^13.0.0",
        "puppeteer": "^5.5.0",
        "puppeteer-core": "^5.5.0",
        "typescript-ioc": "^3.2.2",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/body-parser": "^1.17.1",
        "@types/chai": "^4.2.3",
        "@types/compression": "^1.0.1",
        "@types/cors": "^2.8.9",
        "@types/errorhandler": "^0.0.32",
        "@types/eslint": "^6.1.1",
        "@types/express": "^4.17.1",
        "@types/ioredis": "^4.17.9",
        "@types/jest": "^24.9.1",
        "@types/node": "^12.7.8",
        "@types/puppeteer-core": "^2.0.0",
        "@types/request": "^2.48.3",
        "@types/request-promise": "^4.1.44",
        "@types/supertest": "^2.0.8",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^2.3.1",
        "@typescript-eslint/parser": "^2.3.1",
        "chai": "^4.2.0",
        "concurrently": "^5.3.0",
        "eslint": "^6.4.0",
        "jest": "^26.6.3",
        "kill-port": "^1.6.1",
        "nodemon": "^1.19.2",
        "supertest": "^4.0.2",
        "ts-jest": "^26.4.4",
        "ts-node": "^8.4.1",
        "typescript": "^3.9.7"
    }
}
